<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Action -->
    <record id="action_student" model="ir.actions.act_window">
        <field name="name">Students</field>
        <field name="res_model">student.student</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_student_tree"/>
        <field name="search_view_id" ref="view_student_search"/>
    </record>

    <!-- Menu Item -->
    <menuitem id="menu_student_root" name="Students"/>
    <menuitem id="menu_student" name="Student List" parent="menu_student_root" action="action_student"/>
</odoo>
