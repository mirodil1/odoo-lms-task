<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Action -->
    <record id="action_group" model="ir.actions.act_window">
        <field name="name">Groups</field>
        <field name="res_model">group.group</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_group_tree"/>
        <!-- <field name="search_view_id" ref="view_group_search"/> -->
    </record>

    <!-- Menu Item -->
    <menuitem id="menu_group_root" name="Groups"/>
    <menuitem id="menu_group" name="Group List" parent="menu_group_root" action="action_group"/>
</odoo>
